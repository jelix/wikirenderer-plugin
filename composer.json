{
    "name": "jelix/wikirenderer-plugin",
    "type": "jelix-module",
    "description": "a plugin for Jelix or Castor to render wiki content in a template",
    "keywords": ["wiki", "jelix", "castor", "jtpl"],
    "homepage": "http://jelix.org",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Laurent Jouanneau",
            "email": "laurent@jelix.org"
        }
    ],
    "require": {
        "jelix/wikirenderer": "~3.1.8"
    },
    "autoload": {
        "classmap": [ "lib/" ]
    },
    "minimum-stability": "stable",
    "extra" : {
        "jelix" : {
            "plugins-dir": [
                "plugins/"
            ]
        },
        "branch-alias": {
            "master":"1.7.x-dev"
        }
    }
}
